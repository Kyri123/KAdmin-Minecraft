<div class="content-wrapper d-flex flex-column" style="min-height: 835px;">

    <!-- Beginne eigentl. seite -->
    <section class="content mt-0">
        <nav class="navbar navbar-light navbar-expand border-dark elevation-2 m-0 mb-4 pl-4" style="margin-right: -7.5px!important;;margin-left: -7.5px!important;">
            <a class="navbar-brand" href="#"><i class="<%= icon %>" aria-hidden="true"></i> <%= pagename %></a>

            <ol class="breadcrumb ml-auto m-0 bg-light">
                <li class="breadcrumb-item"><a href="/">ArkAdmin 2</a></li>
                <li class="breadcrumb-item"><%= pagename %></li>
            </ol>
        </nav>

        <div class="container-fluid">

            <section class="content">
                <% if(userHelper.hasPermissions(user.id, "all/show_traffic")) { %>
                    <div class="container-fluid">
                        <!-- Traffic -->
                        <div class="row mb-3">
                            <!-- CPU -->
                            <div class="col-12 col-sm-6 col-md-3">
                                <div class="info-box mb-0 p-0" style="height: 100%">
                                    <span class="info-box-icon bg-info elevation-1"><i class="fas fa-microchip" aria-hidden="true"></i></span>
                                    <div class="info-box-content pt-2 pl-3">
                                        <span class="info-box-text"><%- PANEL_LANG.all.cpu %></span>
                                        <div class="progress">
                                            <div class="progress-bar bg-info"  id="cpu_perc"style="width: 0%"></div>
                                        </div>
                                        <span class="info-box-number" id="cpu">
                                            ?<small>%</small>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <!-- RAM -->
                            <div class="col-12 col-sm-6 col-md-3">
                                <div class="info-box mb-0 p-0" style="height: 100%">
                                    <span class="info-box-icon bg-primary elevation-1"><i class="fas fa-memory" aria-hidden="true"></i></span>
                                    <div class="info-box-content pt-2 pl-3">
                                        <span class="info-box-text"><%- PANEL_LANG.all.ram %></span>
                                        <div class="progress">
                                            <div class="progress-bar bg-primary" id="ram_perc" style="width: 0%"></div>
                                        </div>
                                        <span class="info-box-number" id="ram">
                                            ?
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="clearfix hidden-md-up"></div>
                            <!-- SPEICHER -->
                            <div class="col-12 col-sm-6 col-md-3">
                                <div class="info-box mb-0 p-0" style="height: 100%">
                                    <span class="info-box-icon bg-danger elevation-1"><i class="fas fa-hdd" aria-hidden="true"></i></span>
                                    <div class="info-box-content pt-2 pl-3">
                                        <span class="info-box-text"><%- PANEL_LANG.all.mem %></span>
                                        <div class="progress">
                                            <div class="progress-bar bg-danger" id="mem_perc" style="width: 0%"></div>
                                        </div>
                                        <span class="info-box-number" id="mem">
                                            ?
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <!-- SERV -->
                            <div class="col-12 col-sm-6 col-md-3">
                                <div class="info-box mb-0 p-0" style="height: 100%">
                                    <span class="info-box-icon bg-warning elevation-1"><i class="fas fa-server" aria-hidden="true"></i></span>
                                    <div class="info-box-content pt-2 pl-3">
                                        <span class="info-box-text"><%- PANEL_LANG.all.server %></span>
                                        <div class="progress">
                                            <div class="progress-bar bg-success" id="top_perc" style="width: 0%"></div>
                                        </div>
                                        <span class="info-box-number">
                                            <span class="text-success" id="top_on">?</span> / <span class="text-danger" id="top_off">?</span> / <span class="text-info" id="top_proc">?</span> / <span class="text-primary" id="top_total">?</span>
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    <% } %>

                    <%- isUpdate ? alerter.rd(3001).replace("<branch>", panelBranch) : "" %>
                    <div id="global_resp"><%- resp %></div>