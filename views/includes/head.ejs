        <script src="/plugins/codemirror/codemirror.js"></script>
        <script src="/js/kadmin/functions.js?time=<%- Date.now() %>"></script>
        <script src="/js/kadmin/modalsandtoast.js?time=<%- Date.now() %>"></script>
    </head>