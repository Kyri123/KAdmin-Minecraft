        <style type="text/css">
            /** {
                border-radius: 0px !important;
            }*/
        </style>

        <!-- Extern JS -->
        <script src="/js/vue/vue.js"></script>
        <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js?time=<% Date.now() %>"></script>
        <script src="/jquery/jquery-3.4.1.min.js"></script>
