        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/chart.js@2.9.3/dist/Chart.min.js"></script>

        <!-- Intern JS -->
        <script>
            // Vars die zur JS Ã¼bergeben werden
            // Allgemeine Vars
            let globalvars = {}
            globalvars.alertlang    = <%- lang.alert !== undefined ? JSON.stringify(lang.alert) : {} %>
            globalvars.lang_arr     = <%- lang !== undefined ? JSON.stringify(lang) : {} %>
            globalvars.perm         = <%- perm !== undefined ? JSON.stringify(perm) : {} %>
            globalvars.isUpdate     = <%- isUpdate %>
            globalvars.isUpdating   = <%- isUpdating %>
            globalvars.needRestart  = <%- needRestart %>
            globalvars.branch       = '<%- CONFIG.updater.useBranch %>'

            setInterval(() => {
                try {
                    $.get('/ajax/all?getselfperm', (datas) => {
                        globalvars.perm = JSON.parse(datas)
                    })
                    $.get('/ajax/all?getisupdate', (datas) => {
                       let obj                  = JSON.parse(datas)
                       globalvars.isUpdate      = obj.is_update
                       globalvars.isUpdating    = obj.isUpdating
                       globalvars.needRestart   = obj.needRestart
                    })
                } catch (e) {}
            }, 2000)
        </script>
        <script src="/plugins/jquery/jquery.min.js"></script>
        <script src="/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
        <script src="/plugins/sweetalert2/sweetalert2.min.js"></script>
